{
  "schemaVersion": 1,
  "id": "splinecraft",
  "version": "${version}",
  "name": "Splinecraft",
  "description": "Simulation and creation of RoadRunner trajectories in Minecraft",
  "authors": [
    "Tropheus Jay"
  ],
  "contact": {
    "sources": "https://github.com/TropheusJ/splinecraft"
  },
  "license": "MIT",
  "icon": "assets/splinecraft/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.tropheusj.splinecraft.SplineCraft"
    ],
    "client": [
      "io.github.tropheusj.splinecraft.SplineCraftClient"
    ],
    "fabric-datagen": [
      "io.github.tropheusj.splinecraft.SplineCraftData"
    ]
  },
  "mixins": [
    "splinecraft.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric-api": ">=${fabric_version}",
    "minecraft": ">=${minecraft_version}",
    "java": ">=${java_version}"
  }
}
